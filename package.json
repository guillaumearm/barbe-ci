{
  "name": "sdp",
  "version": "0.5.6",
  "description": "Systeme De Pointage",
  "main": "index.js",
  "bin": [
    "./src/server/index.js"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "build:dev": "NODE_ENV=development webpack",
    "clean": "rimraf ./dist ./coverage ./e2e/dist",
    "start": "NODE_ENV=production node ./src/server -p 8000",
    "dev": "NODE_ENV=development nodemon -w ./src/server ./src/server/index.js -p 8080 --verbose",
    "lint": "eslint .",
    "test": "NODE_ENV=test jest --config src/client/jest.config.js",
    "test:coverage": "NODE_ENV=test jest --config src/client/jest.config.js --coverage",
    "test:watch": "NODE_ENV=test jest --config src/client/jest.config.js --watch",
    "test:integration": "NODE_ENV=test jest -i --config e2e/jest.config.js",
    "test:all": "npm run test && npm run test:integration && npm run lint",
    "precommit": "lint-staged",
    "prepush": "npm run test:all",
    "preversion": "npm run test:all",
    "help": "node node_modules/npm-scripts-help"
  },
  "scriptshelp": {
    "build": "build the project for production",
    "build:dev": "build the project for development",
    "clean": "clean dist/ folder",
    "start": "start production server (default port to 8000)",
    "dev": "start dev server (default port to 8080)",
    "lint": "lint all files in src/ folder",
    "test": "start jest",
    "test:coverage": "start jest in coverage mode",
    "test:watch": "start jest in watch mode",
    "test:integration": "start end-to-end integration tests",
    "test:all": "start unit tests, inegration tests, then lint tests",
    "precommit": "git precommit hook",
    "prepush": "git prepush hook",
    "preversion": "git preversion hook",
    "help": "show this help"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/trapcodien/sdp.git"
  },
  "keywords": [
    "pointing",
    "system"
  ],
  "author": "Guillaume ARM",
  "license": "ISC",
  "homepage": "https://bitbucket.org/trapcodien/sdp#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-react-css-modules": "^3.3.2",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-react": "^7.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "koa-webpack": "^1.0.0",
    "lint-staged": "^5.0.0",
    "nodemon": "^1.12.1",
    "npm-scripts-help": "^0.7.0",
    "path": "^0.12.7",
    "puppeteer": "^0.13.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0"
  },
  "dependencies": {
    "express-to-koa": "^1.0.6",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.1.0",
    "koa-rewrite": "^3.0.0",
    "koa-router": "^7.3.0",
    "koa-static-cache": "^5.1.1",
    "ramda": "^0.25.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "yargs": "^10.0.3"
  },
  "engines": {
    "node": ">=7.10.0"
  }
}
