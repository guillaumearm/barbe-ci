{
  "name": "sdp",
  "version": "0.6.4",
  "description": "Systeme De Pointage",
  "scripts": {
    "dev": "cd packages/client && npm run dev",
    "lint:client": "cd packages/client && npm run lint",
    "lint:server": "cd packages/server && npm run lint",
    "lint": "npm run lint:client && npm run lint:server",
    "preinstall": "npm run install:client && npm run install:server",
    "install:client": "cd packages/client && npm install",
    "install:server": "cd packages/server && npm install",
    "precommit:client": "cd packages/client && npm run lint-staged",
    "precommit:server": "cd packages/server && npm run lint-staged",
    "precommit": "npm-run-all precommit:*",
    "help": "node node_modules/npm-scripts-help"
  },
  "scriptshelp": {
    "lint:client": "Lint client files",
    "lint:server": "Lint client files",
    "lint": "lint all project files (client and server)",
    "help": "show this help"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/trapcodien/sdp.git"
  },
  "keywords": [
    "pointing",
    "system"
  ],
  "author": "Guillaume ARM",
  "license": "ISC",
  "homepage": "https://bitbucket.org/trapcodien/sdp#readme",
  "devDependencies": {
    "husky": "^0.14.3",
    "npm-run-all": "^4.1.2",
    "npm-scripts-help": "^0.7.0"
  },
  "dependencies": {}
}
